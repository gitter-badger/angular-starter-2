{
  "name": "angular-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node .build/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node .build/srcServer.js",
    "open:dist": "babel-node .build/distServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src .build",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress .build/testSetup \"src/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch",
    "remove-dist": "rimraf ./dist",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node .build/build.js && npm run open:dist"
  },
  "author": "Mykhailo Petrenko",
  "repository": "https://github.com/mykhailo-petrenko/angular-starter.git",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "6.8.0",
    "bootstrap": "3.3.6",
    "jquery": "2.2.3",
    "angular": "",
    "toastr": "2.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-register": "6.8.0",
    "chai": "^3.5.0",
    "colors": "1.1.2",
    "compression": "1.6.1",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "eslint": "2.9.0",
    "eslint-plugin-chai-friendly": "^0.2.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-watch": "2.1.11",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.19.0",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "^2.26.0",
    "jsdom": "8.5.0",
    "mocha": "2.4.5",
    "nock": "8.0.0",
    "node-sass": "^4.3.0",
    "npm-run-all": "1.8.0",
    "open": "0.0.5",
    "raw-loader": "^0.5.1",
    "rimraf": "2.5.2",
    "sass-loader": "^4.1.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  }
}
